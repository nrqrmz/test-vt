<ul>
  <% @psychologists.each do |psych| %>
    <li>
      <%= psych.first_name %> <%= psych.flast_name %>
    </li>
  <% end %>
</ul>

<%= form_with url: all_path, method: :get, class: "d-flex" do %>
  <%= text_field_tag :query,
    params[:query],
    class: "form-control",
    placeholder: "Type something here"
  %>
  <%= submit_tag "Search", class: "btn btn-primary" %>
<% end %>

